# Environment Configuration Template
# Copy this file to .env and fill in your values
# DO NOT commit .env file with real credentials

# Build Configuration
GENERATE_SOURCEMAP=false
DISABLE_ESLINT_PLUGIN=false

# API Configuration (add your API endpoints here)
# REACT_APP_API_URL=http://localhost:3000
# REACT_APP_SOCKET_URL=http://localhost:3000

# Authentication (add your auth config here if needed)
# REACT_APP_JWT_SECRET=your-secret-here

# Third-party Services (add your keys here if needed)
# REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id
# REACT_APP_GITHUB_CLIENT_ID=your-github-client-id

# Feature Flags
# REACT_APP_ENABLE_ANALYTICS=false
# REACT_APP_ENABLE_DEBUG=false
